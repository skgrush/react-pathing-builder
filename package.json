{
  "name": "react-pathing-builder",
  "version": "0.1.0",
  "description": "A React component for building pathing graphs on maps.",
  "homepage": "https://skgrush.github.io/react-pathing-builder",
  "main": "lib/react-pathing-builder.js",
  "types": "lib/",
  "repository": {
    "type": "git",
    "url": "https://github.com/skgrush/react-pathing-builder.git"
  },
  "author": "Samuel K. Grush",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "test": "jest --config jestconfig.json",
    "build": "yarn clean:build && yarn build:types && yarn build:js && yarn build:css",
    "build:js": "webpack --config webpack.build.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:css": "node-sass src/styles.scss lib/styles.css",
    "babel:build:js": "babel src -o lib/index.js --extensions '.ts,.tsx' --source-maps inline",
    "clean:build": "rm -rf lib",
    "clean:nm": "rm -rf node_modules",
    "type-check": "tsc --noEmit",
    "prebulishOnly": "yarn test",
    "prepare": "yarn build",
    "build:dist": "webpack --config webpack.dist.js",
    "cp:dist": "cp examples/src/* examples/dist/",
    "predeploy": "yarn cp:dist && yarn build:dist",
    "deploy": "gh-pages -d examples/dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/webpack-env": "^1.13.6",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "css-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.15.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.2.1"
  }
}
